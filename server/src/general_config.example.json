{
  "dialog_graphs": {
    "Isadore Graph": {
      "name_match": "^Bin ",
      "queries": [
        {
          "time": {
            "begin_datetime": "$begin_datetime$",
            "end_datetime": "$end_datetime$",
            "sample_period": 5
          },
          "labels": {
            "title": "$bin_name$",
            "y0label": "<span style=\"color: #FF0000\">&deg;F</span>",
            "y1label": "<span style=\"color: #0010A5\">%</span>"
          },
          "data": [
            {
              "value": "$bin_name$.inlet.Temp",
              "display": {
                "label": "Inlet T",
                "color": "#FF0000",
                "pattern": null,
                "axis": 0
              }
            },
            {
              "value": "$bin_name$.outlet.Temp",
              "display": {
                "label": "Outlet T",
                "color": "#FF0000",
                "pattern": [
                  2,
                  2
                ],
                "axis": 0
              }
            },
            {
              "value": "$bin_name$.inlet.RH",
              "display": {
                "label": "Inlet RH",
                "color": "#0010A5",
                "pattern": null,
                "axis": 1
              }
            },
            {
              "value": "$bin_name$.outlet.RH",
              "display": {
                "label": "Outlet RH",
                "color": "#0010A5",
                "pattern": [
                  2,
                  2
                ],
                "axis": 1
              }
            }
          ]
        }
      ],
      "options": {
        "total_height": 650
      }
    },
    "WB Graph": {
      "name_match": "^Bin ",
      "queries": [
        {
          "time": {
            "begin_datetime": "$begin_datetime$",
            "end_datetime": "$end_datetime$",
            "sample_period": 5,
            "time_display": "hours"
          },
          "labels": {
            "y0label": "&deg;F",
            "y1label": "y 1 label"
          },
          "data": [
            {
              "value": "$bin_name$.Top.Temp",
              "display": {
                "label": "Top Temp",
                "color": "#FF0000",
                "pattern": null,
                "axis": 0
              }
            },
            {
              "value": "$bin_name$.Bottom.Temp",
              "display": {
                "label": "Bottom Temp",
                "color": "#bf5f00",
                "pattern": null,
                "axis": 0
              }
            },
            {
              "value": "$bin_name$.outlet.Wet Bulb",
              "display": {
                "label": "Outlet WB",
                "color": "#003d1d",
                "gg": null,
                "axis": 0
              }
            }
          ]
        }
      ],
      "options": {
        "total_height": 650,
        "heights": [
          0.6,
          0.4
        ]
      }
    },
    "MCP Graph": {
      "name_match": "^Bin ",
      "queries": [
        {
          "time": {
            "begin_datetime": "$begin_datetime$",
            "end_datetime": "$end_datetime$",
            "sample_period": 15
          },
          "labels": {
            "title": "$bin_name$",
            "y0label": "%"
          },
          "initial_options": {
            "y0range": [
              0,
              20
            ]
          },
          "data": [
            {
              "value": "$bin_name$.Top.MCP A",
              "display": {
                "label": "MCP A",
                "color": "#404000",
                "pattern": null,
                "axis": 0
              }
            },
            {
              "value": "$bin_name$.Top.MCP B",
              "display": {
                "label": "MCP B",
                "color": "#0000FF",
                "pattern": null,
                "axis": 0
              }
            },
            {
              "value": "$bin_name$.Top.MCP C",
              "display": {
                "label": "MCP C",
                "color": "#FF0000",
                "pattern": null,
                "axis": 0
              }
            }
          ]
        },
        {
          "time": {
            "begin_datetime": "$begin_datetime$",
            "end_datetime": "$end_datetime$",
            "sample_period": 15
          },
          "labels": {
            "y0label": "%/hr"
          },
          "initial_options": {
            "y0range": [
              0,
              20
            ]
          },
          "data": [
            {
              "value": "$bin_name$.Top.dMCP/dt A",
              "display": {
                "label": "dMCP/dt A",
                "color": "#404000",
                "pattern": null,
                "axis": 0
              }
            },
            {
              "value": "$bin_name$.Top.dMCP/dt B",
              "display": {
                "label": "dMCP/dt B",
                "color": "#0000FF",
                "pattern": null,
                "axis": 0
              }
            },
            {
              "value": "$bin_name$.Top.dMCP/dt C",
              "display": {
                "label": "dMCP/dt C",
                "color": "#FF0000",
                "pattern": null,
                "axis": 0
              }
            }
          ]
        }
      ],
      "options": {
        "total_height": 650,
        "heights": [
          0.6,
          0.4
        ]
      }
    },
    "Test Graph2": {
      "name_match": "^Bin ",
      "queries": [
        {
          "time": {
            "begin_datetime": "$begin_datetime$",
            "end_datetime": "$end_datetime$",
            "sample_period": 5
          },
          "labels": {
            "title": "$bin_name$",
            "y0label": "<span style=\"color: #FF0000\">&deg;F</span>",
            "y1label": "<span style=\"color: #000000\">g/m^3</span>"
          },
          "data": [
            {
              "value": "$bin_name$.inlet.Temp",
              "vname": "itemp",
              "display": {
                "label": "Inlet Temp",
                "color": "#FF0000",
                "pattern": null,
                "axis": 0
              }
            },
            {
              "value": "$bin_name$.outlet.Temp",
              "vname": "otemp",
              "display": {
                "label": "Outlet Temp",
                "color": "#FF0000",
                "pattern": [
                  2,
                  2
                ],
                "axis": 0
              }
            },
            {
              "value": "$bin_name$.inlet.abshum",
              "vname": "iabshum",
              "display": {
                "label": "Inlet AbsH",
                "color": "#000000",
                "pattern": [
                  7,
                  2
                ],
                "axis": 1
              }
            },
            {
              "value": "$bin_name$.outlet.abshum",
              "vname": "oabshum",
              "display": {
                "label": "Outlet AbsH",
                "color": "#000000",
                "pattern": [
                  2,
                  2
                ],
                "axis": 1
              }
            }
          ],
          "calc": [
            {
              "value": "oabshum-iabshum",
              "display": {
                "label": "dAbsH",
                "color": "#000000",
                "pattern": null,
                "axis": 1
              },
              "options": {
                "all_rows": true
              }
            }
          ]
        }
      ],
      "options": {
        "total_height": 650
      }
    }
  },
